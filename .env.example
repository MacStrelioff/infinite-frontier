# Venice AI API
VENICE_AI_API_KEY=your_venice_api_key_here

# OpenSea API (optional - bids won't display without it)
OPENSEA_API_KEY=your_opensea_api_key_here

# WalletConnect Project ID (optional - only needed for mobile wallet QR code connections)
# Browser extension wallets (MetaMask, Coinbase Wallet, etc.) work without this
# Get free project ID at: https://cloud.walletconnect.com
NEXT_PUBLIC_WALLET_CONNECT_PROJECT_ID=

# Blockchain configuration
BASE_RPC_URL=https://mainnet.base.org
BASE_SEPOLIA_RPC_URL=https://sepolia.base.org
PRIVATE_KEY=your_private_key_for_deployment

# Contract verification
BASESCAN_API_KEY=your_basescan_api_key

# Optional: Gas reporting
REPORT_GAS=true
